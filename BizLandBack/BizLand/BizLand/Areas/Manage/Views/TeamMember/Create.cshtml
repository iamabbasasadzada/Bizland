@model TeamMember
<p>Create Team Member</p>
<form method="post" enctype="multipart/form-data" asp-action="Create">
    <div class="mb-3">
        <label asp-for="FullName" class="form-label"></label>
        <input asp-for="FullName" class="form-control" placeholder="FullName">
        <span asp-validation-for="FullName" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Position" class="form-label"></label>
        <input asp-for="Position" class="form-control" placeholder="Position">
        <span asp-validation-for="Position" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="FacebookLink" class="form-label"></label>
        <input asp-for="FacebookLink" class="form-control" placeholder="FacebookLink">
        <span asp-validation-for="FacebookLink" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="InstaLink" class="form-label"></label>
        <input asp-for="InstaLink" class="form-control" placeholder="InstaLink">
        <span asp-validation-for="InstaLink" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="TwitterLink" class="form-label"></label>
        <input asp-for="TwitterLink" class="form-control" placeholder="TwitterLink">
        <span asp-validation-for="TwitterLink" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="LinkedinLink" class="form-label"></label>
        <input asp-for="LinkedinLink" class="form-control" placeholder="LinkedinLink">
        <span asp-validation-for="LinkedinLink" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Photo" class="form-label"></label>
        <input asp-for="Photo" class="form-control">
        <span asp-validation-for="Photo" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <button type="submit" class="btn btn-primary">Create</button>
    </div>
</form>